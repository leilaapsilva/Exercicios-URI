//https://www.urionlinejudge.com.br/judge/pt/problems/view/1060

#include <iostream>
  
using namespace std;
  
int main() {
  
    float valor; 
    int i, soma = 0;
     
    for(i = 0; i < 6; i ++)
    {
        scanf("%f", &valor);
        if (valor > 0){
            soma = soma + 1;
        }
    } 
    printf("%d valores positivos\n", soma);
     
  
    return 0;
}
